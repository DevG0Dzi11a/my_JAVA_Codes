<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="home.PlayFXMLController">
   <children>
      <ImageView fitHeight="700.0" fitWidth="1100.0" pickOnBounds="true">
         <image>
            <Image url="@../image/31189.png" />
         </image>
      </ImageView>
      <TextField fx:id="printbox" editable="false" layoutX="233.0" layoutY="196.0" prefHeight="80.0" prefWidth="600.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </TextField>
      <Button fx:id="showAns" layoutX="439.0" layoutY="320.0" mnemonicParsing="false" onAction="#showAction" prefHeight="60.0" prefWidth="231.0" text="Show Answer">
         <font>
            <Font name="Blade Singer" size="23.0" />
         </font>
         <effect>
            <SepiaTone />
         </effect>
      </Button>
      <Button fx:id="openFile" layoutX="18.0" layoutY="27.0" mnemonicParsing="false" onAction="#openAction" prefHeight="60.0" prefWidth="231.0" text="Open file">
         <font>
            <Font name="Blade Singer" size="23.0" />
         </font>
         <effect>
            <SepiaTone />
         </effect>
      </Button>
      <Button layoutX="21.0" layoutY="629.0" mnemonicParsing="false" onAction="#homeAction" prefHeight="57.0" prefWidth="96.0" text="HOME">
         <font>
            <Font name="Blade Singer" size="20.0" />
         </font>
         <effect>
            <SepiaTone />
         </effect>
      </Button>
   </children>
</AnchorPane>
